{
  "name": "xenosite-vercel",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "deploy": "yarn build && npx vercel --prod",
    "clean": "rm -fr public/build",
    "build:remix": "remix build",
    "build:rdkit": "cp -r ./node_modules/@rdkit/rdkit/Code/MinimalLib/dist rdkit && cp rdkit/*.wasm api",
    "build": "yarn build:css && remix build ",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "dev:remix": "remix dev",
    "dev": "concurrently \"yarn dev:css\" \"yarn dev:remix\"",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "create-sitemaps": "node scripts/create-sitemaps.js",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@babel/preset-react": "^7.22.15",
    "@headlessui/react": "^1.7.17",
    "@remix-run/css-bundle": "^2.0.1",
    "@remix-run/node": "^2.0.1",
    "@remix-run/react": "^2.0.1",
    "@remix-run/serve": "^2.0.1",
    "@remix-run/vercel": "^1.19.3",
    "@tailwindcss/typography": "^0.5.10",
    "@vercel/node": "^2.15.10",
    "dotenv": "^16.3.1",
    "isbot": "^3.7.0",
    "opentype": "^0.1.2",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-recompose": "^0.33.0",
    "satori": "^0.10.9",
    "smiles-drawer": "^2.1.7",
    "svg2img": "^1.0.0-beta.2",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@remix-run/dev": "^2.0.1",
    "@remix-run/eslint-config": "^2.0.1",
    "@types/msgpack5": "^3.4.6",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "autoprefixer": "^10.4.16",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.1",
    "eslint": "^8.38.0",
    "msgpack5": "^6.0.2",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "version": "0.0.0"
}
