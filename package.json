{
  "name": "xenosite-vercel",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "deploy": "yarn build && npx vercel --prod",
    "clean": "rm -fr public/build",
    "build:remix": "remix build",
    "postinstall": "yarn remix setup node",
    "build:rdkit": "cp -r ./node_modules/@rdkit/rdkit/Code/MinimalLib/dist rdkit && cp rdkit/*.wasm api",
    "build": "yarn build:css && remix build ",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "dev:remix": "remix dev",
    "dev": "concurrently \"yarn dev:css\" \"yarn dev:remix\"",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.58.0",
    "@aws-sdk/client-lambda": "^3.58.0",
    "@aws-sdk/util-utf8-node": "^3.55.0",
    "@babel/preset-react": "^7.18.6",
    "@headlessui/react": "^1.7.3",
    "@heroicons/react": "^2.0.12",
    "@remix-run/react": "^1.3.3",
    "@remix-run/vercel": "^1.7.2",
    "@tailwindcss/typography": "^0.5.7",
    "@vercel/node": "^1.8.3",
    "aws-sdk": "^2.1112.0",
    "clean-css": "^5.2.4",
    "dotenv": "^16.0.0",
    "isbot": "^3.6.0",
    "opentype": "^0.1.2",
    "opentype.js": "^1.3.4",
    "prettier": "^2.6.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recompose": "^0.30.0",
    "remix": "^1.7.4",
    "satori": "^0.0.43",
    "smiles-drawer": "^2.0.3",
    "yarn": "^1.22.19",
    "zora": "^5.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@remix-run/dev": "^1.3.2",
    "@remix-run/eslint-config": "^1.3.2",
    "@remix-run/serve": "^1.3.2",
    "autoprefixer": "^10.4.4",
    "babel-loader": "^8.2.4",
    "concurrently": "^7.0.0",
    "eslint": "^8.11.0",
    "postcss": "^8.4.12",
    "tailwindcss": "^3.0.23"
  },
  "engines": {
    "node": ">=14"
  }
}
