{
  "name": "remix-template-vercel",
  "private": true,
  "description": "",
  "license": "",
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "clean": "rm -r public/build",
    "build:remix": "remix build",
    "dev:remix": "remix dev",
    "postinstall": "yarn patch-package && yarn remix setup node",
    "build": "yarn build:css && remix build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "dev": "concurrently \"yarn dev:css\" \"yarn dev:remix\"",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css"
  },
  "dependencies": {
    "@rdkit/rdkit": "^2021.9.5",
    "@remix-run/react": "^1.3.3",
    "@remix-run/vercel": "^1.3.2",
    "jsdom": "^19.0.0",
    "linkedom": "^0.14.5",
    "openchemlib": "^7.4.3",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.3.2",
    "smiles-drawer": "https://github.com/reymond-group/smilesDrawer"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.3.2",
    "@remix-run/eslint-config": "^1.3.2",
    "@remix-run/serve": "^1.3.2",
    "autoprefixer": "^10.4.4",
    "concurrently": "^7.0.0",
    "eslint": "^8.11.0",
    "postcss": "^8.4.12",
    "serverless-python-requirements": "^5.4.0",
    "tailwindcss": "^3.0.23"
  },
  "engines": {
    "node": ">=14"
  }
}
